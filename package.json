{
  "name": "anarchy.txt",
  "version": "1.0.0",
  "description": "A Massively Multiplayer PVP text file",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsnd src --ignore-watch=player.json --ignore-watch=world.json",
    "client": "source ./.env && tsnd src/client",
    "server": "source ./.env && tsnd src/server",
    "types:refresh": "source ./.env && openapi-typescript $SUPABASE_URL/?apikey=$SUPABASE_KEY --output ./src/core/models.ts"
  },
  "author": "Damien Buty <dam.buty@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.9",
    "@types/node-fetch": "2.x",
    "@types/restify": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "openapi-typescript": "^5.1.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.29.2",
    "chalk": "^4.1.2",
    "knuth-shuffle-seeded": "^1.0.6",
    "lodash": "^4.17.21",
    "murmurhash": "^2.0.0",
    "node-fetch": "^2.6.1",
    "restify": "^8.6.0",
    "simplex-noise": "^3.0.1",
    "wcwidth": "^1.0.1"
  }
}
